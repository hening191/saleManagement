<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  


<urlrewrite>
	
	<rule>  
		<note> 登录页 </note>
		<from>^/(\w+)/login.(\w+).html$</from>
		<to>/$1/login_$2.action</to>
	</rule>

	<rule>  
		<note> 工作页 </note>
		<from>^/(\w+)/sale.(\w+).html$</from>
		<to>/$1/sale_$2.action</to>
	</rule>
</urlrewrite> 